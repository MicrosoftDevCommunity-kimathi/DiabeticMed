@model DiabeticProject.Models.MedicPatientRepo.PatientsMEdListviewModel

<table class="table table-dark">
	<thead>
		<tr>
			<th scope="col">#</th>
			<th scope="col">Name</th>
			<th scope="col">Age</th>
			<th scope="col">IsDiabetic</th>
		</tr>
	</thead>
	@foreach (var p in Model.Patients)
	{
		<tr>
			<td scope="row">@p.Id</td>
			<td>@p.Name</td>
			<td>@p.Age</td>
			<td>@p.Isdiabetic</td>
		</tr>
	}
	</table>

	<div page-model="@Model.PagingInfo" page-action="List"></div>


